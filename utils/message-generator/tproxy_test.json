{
    "doc": [
        "This test do:",
            "1) launch bitcoind in regtest and wait for TP initialization",
            "2) create a bunch of block using bictoin-cli",
            "3) launch SRI pool and wait for initialization",
            "4) launch TProxy and wait for initialization with pool",
            "5) launch cpuminer instance on 'downstream' TProxy port",
            "6) wait for cpuminer to send a mining.notify message"
    ],
    "common_messages": [
    ],
    "mining_messages": [
    ],
    "frame_builders": [
    ],
    "actions": [
    ],
    "setup_commands": [
        {
            "command": "cargo",
            "args": [
                        "run",
                        "-p",
                        "pool",
                        "--",
                        "-c",
                        "./roles/v2/pool/pool-config.toml"
            ],
            "conditions": {
                "WithConditions": {
                    "conditions": [
                        {
                            "output_string": "Listening for encrypted connection on: 0.0.0.0:34254",
                            "output_location": "StdOut",
                            "condition": true
                        }
                    ],
                    "timer_secs": 60,
                    "warn_no_panic": false
                }
            }
        },
        {
            "command": "cargo",
            "args": [
                        "run",
                        "-p",
                        "translator",
                        "--",
                        "-c",
                        "./roles/translator/proxy-config.toml"
            ],
            "conditions": {
                "WithConditions": {
                    "conditions": [
                        {
                            "output_string": "PROXY SERVER - ACCEPTING FROM UPSTREAM: 127.0.0.1:34254",
                            "output_location": "StdOut",
                            "condition": true
                        }
                    ],
                    "timer_secs": 60,
                    "warn_no_panic": false
                }
            }
        },
        {
            "command": "/home/brent/code/bitcoin_stuff/cpuminer/minerd",
            "args": [
                        "-o",
                        "stratum+tcp://localhost:34255",
                        "-t",
                        "1",
                        "-q",
                        "-D",
                        "-P",
                        "-a",
                        "sha256d",
                        "-R",
                        "10"
            ],
            "conditions": {
                "WithConditions": {
                    "conditions": [
                        {
                            "output_string": "\"method\":\"mining.subscribe\"",
                            "output_location": "StdErr",
                            "condition": true
                        }
                    ],
                    "timer_secs": 90,
                    "warn_no_panic": false
                }
            }
        }
    ],
    "execution_commands": [
    ],
    "cleanup_commands": [
    ],
    "role": "None"
}