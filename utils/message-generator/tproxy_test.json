{
    "doc": [
        "This test do:",
            "cpuminer = https://github.com/pooler/cpuminer",
            "1) launch SRI pool and wait for initialization",
            "2) launch TProxy and wait for initialization with pool",
            "3) launch 2 cpuminer instances on 'downstream' TProxy port",
            "4) wait for cpuminer to submit 5 blocks"
    ],
    "common_messages": [
    ],
    "mining_messages": [
    ],
    "frame_builders": [
    ],
    "actions": [
    ],
    "setup_commands": [
        {
            "command": "cargo",
            "args": [
                        "run",
                        "-p",
                        "pool",
                        "--",
                        "-c",
                        "./roles/v2/pool/pool-config.toml"
            ],
            "conditions": {
                "WithConditions": {
                    "conditions": [
                        {
                            "output_string": "Listening for encrypted connection on: 0.0.0.0:34254",
                            "output_location": "StdOut",
                            "condition": true
                        }
                    ],
                    "timer_secs": 60,
                    "warn_no_panic": false
                }
            }
        },
        {
            "command": "cargo",
            "args": [
                        "run",
                        "-p",
                        "translator",
                        "--",
                        "-c",
                        "./roles/translator/proxy-config.toml"
            ],
            "conditions": {
                "WithConditions": {
                    "conditions": [
                        {
                            "output_string": "Connected to Upstream!",
                            "output_location": "StdOut",
                            "condition": true
                        }
                    ],
                    "timer_secs": 60,
                    "warn_no_panic": false
                }
            }
        },
        {
            "command": "/home/brent/code/bitcoin_stuff/cpuminer/minerd",
            "args": [
                        "-o",
                        "stratum+tcp://localhost:34255",
                        "-t",
                        "1",
                        "-q",
                        "-D",
                        "-P",
                        "-a",
                        "sha256d",
                        "-R",
                        "10"
            ],
            "conditions": "None"
        },
        {
            "command": "/home/brent/code/bitcoin_stuff/cpuminer/minerd",
            "args": [
                        "-o",
                        "stratum+tcp://localhost:34255",
                        "-t",
                        "1",
                        "-q",
                        "-D",
                        "-P",
                        "-a",
                        "sha256d",
                        "-R",
                        "10"
            ],
            "conditions": {
                "WithConditions": {
                    "conditions": [
                        {
                            "output_string": "accepted: 5/5 ",
                            "output_location": "StdErr",
                            "condition": true
                        }
                    ],
                    "timer_secs": 600,
                    "warn_no_panic": false
                }
            }
        }
    ],
    "execution_commands": [
    ],
    "cleanup_commands": [
    ],
    "role": "None"
}